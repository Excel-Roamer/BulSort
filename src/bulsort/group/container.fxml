<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="96.0" prefWidth="200.0" styleClass="group-label" stylesheets="@../resources/styles/group-label.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bulsort.group.ContainerController">
   <children>
      <Label fx:id="group" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" textAlignment="CENTER" VBox.vgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <HBox alignment="CENTER_LEFT" prefHeight="32.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="pv" layoutX="15.0" layoutY="15.0" prefHeight="32.0" prefWidth="32.0" text="Label" visible="false">
               <tooltip>
                  <Tooltip fx:id="pvToolTip" text="" />
               </tooltip></Label>
            <Label fx:id="pdf" prefHeight="32.0" prefWidth="32.0" text="Label" visible="false">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <tooltip>
                  <Tooltip fx:id="pdfToolTip" text="" />
               </tooltip></Label>
            <Label fx:id="temp" layoutX="10.0" layoutY="10.0" prefHeight="32.0" prefWidth="32.0" text="Label" visible="false">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <tooltip>
                  <Tooltip fx:id="tempToolTip" text="%TEMPORARY_TOOLTIP" />
               </tooltip></Label>
            <StackPane prefHeight="32.0" prefWidth="32.0">
               <children>
                  <Label fx:id="sorted" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" visible="false">
                     <tooltip>
                        <Tooltip fx:id="sortedToolTip" text="%SORTED_TOOLTIP" />
                     </tooltip></Label>
                  <ProgressIndicator fx:id="progress" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" visible="false">
                     <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                     </padding></ProgressIndicator>
               </children>
            </StackPane>
            <Label fx:id="size" alignment="CENTER_RIGHT" layoutX="74.0" layoutY="10.0" maxWidth="1.7976931348623157E308" prefHeight="32.0" text="Label" textAlignment="CENTER" HBox.hgrow="ALWAYS">
                <tooltip>
                  <Tooltip fx:id="sizeToolTip" text="" />
               </tooltip>
            </Label>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</VBox>
